{
  "redirects": [
    {
      "source": "/:path*",          // match any path
      "has": [
        {
          "type": "host",
          "value": "www.whichslot.com"   // only when host is www
        }
      ],
      "destination": "https://whichslot.com/:path*",
      "permanent": true
    }
  ]
}
